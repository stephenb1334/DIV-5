{
  "API_Documentation": {
    "Base_URL": "https://api-atlas.nomic.ai",
    "Authentication": {
      "Method": "Bearer Token",
      "Header_Example": {
        "Authorization": "Bearer <TOKEN>"
      }
    },
    "Endpoint": {
      "Name": "Create API Key",
      "Method": "POST",
      "Path": "/v1/user/authorization/keys/:organization_id/create",
      "Description": "Creates a new API key with specified scope and permissions.",
      "Parameters": {
        "Path_Parameters": {
          "organization_id": {
            "Description": "The unique identifier (UUID) of the organization.",
            "Type": "string",
            "Required": true
          }
        },
        "Body_Parameters": {
          "key_name": {
            "Description": "The name of the API key to create.",
            "Type": "string",
            "Required": true
          },
          "key_role": {
            "Description": "The role for the API key (e.g., OWNER).",
            "Type": "string",
            "Required": true
          },
          "key_scope": {
            "Description": "The scope of the API key.",
            "Type": "enum",
            "Possible_Values": ["ORGANIZATION", "DATASET", "USER"],
            "Required": true
          },
          "key_target_id": {
            "Description": "The UUID representing a dataset or organization.",
            "Type": "string",
            "Required": false
          }
        }
      },
      "Responses": {
        "200": {
          "Description": "Successful response.",
          "Schema": {
            "key": {
              "Description": "The API key that was created.",
              "Type": "string",
              "Required": true
            }
          },
          "Example_Response": {
            "key": "string"
          }
        },
        "422": {
          "Description": "Validation Error.",
          "Schema": {
            "detail": [
              {
                "loc": [
                  "string",
                  0
                ],
                "msg": "string",
                "type": "string"
              }
            ]
          },
          "Example_Response": {
            "detail": [
              {
                "loc": [
                  "string",
                  0
                ],
                "msg": "string",
                "type": "string"
              }
            ]
          }
        }
      }
    },
    "Examples": {
      "CURL": "curl -L 'https://api-atlas.nomic.ai/v1/user/authorization/keys/:organization_id/create' \\\n-H 'Content-Type: application/json' \\\n-H 'Accept: application/json' \\\n-H 'Authorization: Bearer <TOKEN>' \\\n-d '{\n  \"key_name\": \"string\",\n  \"key_role\": \"OWNER\",\n  \"key_scope\": \"ORGANIZATION\",\n  \"key_target_id\": \"3fa85f64-5717-4562-b3fc-2c963f66afa6\"\n}'",
      "Python": "import requests\nimport json\n\nurl = \"https://api-atlas.nomic.ai/v1/user/authorization/keys/:organization_id/create\"\n\npayload = json.dumps({\n  \"key_name\": \"string\",\n  \"key_role\": \"OWNER\",\n  \"key_scope\": \"ORGANIZATION\",\n  \"key_target_id\": \"3fa85f64-5717-4562-b3fc-2c963f66afa6\"\n})\nheaders = {\n  'Content-Type': 'application/json',\n  'Accept': 'application/json',\n  'Authorization': 'Bearer <TOKEN>'\n}\n\nresponse = requests.request(\"POST\", url, headers=headers, data=payload)\n\nprint(response.text)"
    }
  }
}