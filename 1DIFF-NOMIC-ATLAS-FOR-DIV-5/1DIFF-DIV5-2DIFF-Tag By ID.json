{
  "API_Documentation": {
    "Base_URL": "https://api-atlas.nomic.ai",
    "Authentication": {
      "Method": "Bearer Token",
      "Header_Example": {
        "Authorization": "Bearer <TOKEN>"
      }
    },
    "Endpoint": {
      "Name": "Tag By ID",
      "Method": "POST",
      "Path": "/v1/tags/id",
      "Description": "Applies tags to specific dataset entries identified by their unique IDs.",
      "Parameters": {
        "Body_Parameters": {
          "dataset_id": {
            "Description": "The unique identifier of the dataset.",
            "Type": "string",
            "Required": true
          },
          "entry_ids": {
            "Description": "List of entry IDs to apply tags to.",
            "Type": "array",
            "Required": true
          },
          "tags": {
            "Description": "List of tags to apply.",
            "Type": "array",
            "Required": true
          }
        }
      },
      "Responses": {
        "200": {
          "Description": "Successful response with applied tags.",
          "Schema": {
            "tags_applied": {
              "Description": "List of tags successfully applied.",
              "Type": "array",
              "Items": {
                "Type": "string"
              }
            }
          },
          "Example_Response": {
            "tags_applied": ["tag1", "tag2"]
          }
        },
        "422": {
          "Description": "Validation Error.",
          "Schema": {
            "detail": [
              {
                "loc": [
                  "string",
                  0
                ],
                "msg": "string",
                "type": "string"
              }
            ]
          },
          "Example_Response": {
            "detail": [
              {
                "loc": [
                  "string",
                  0
                ],
                "msg": "string",
                "type": "string"
              }
            ]
          }
        }
      }
    },
    "Examples": {
      "CURL": "curl -L 'https://api-atlas.nomic.ai/v1/tags/id' \\\n-H 'Content-Type: application/json' \\\n-H 'Authorization: Bearer <TOKEN>' \\\n-d '{\n  \"dataset_id\": \"string\",\n  \"entry_ids\": [\"id1\", \"id2\"],\n  \"tags\": [\"tag1\", \"tag2\"]\n}'",
      "Python": "import requests\nimport json\n\nurl = \"https://api-atlas.nomic.ai/v1/tags/id\"\n\npayload = json.dumps({\n  \"dataset_id\": \"string\",\n  \"entry_ids\": [\"id1\", \"id2\"],\n  \"tags\": [\"tag1\", \"tag2\"]\n})\nheaders = {\n  'Content-Type': 'application/json',\n  'Authorization': 'Bearer <TOKEN>'\n}\n\nresponse = requests.request(\"POST\", url, headers=headers, data=payload)\n\nprint(response.text)"
    }
  }
}