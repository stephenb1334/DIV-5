{
  "API_Documentation": {
    "Base_URL": "https://api-atlas.nomic.ai",
    "Authentication": {
      "Method": "Bearer Token",
      "Header_Example": {
        "Authorization": "Bearer <TOKEN>"
      }
    },
    "Endpoint": {
      "Name": "Manage Maps",
      "Method": "GET",
      "Path": "/v1/maps",
      "Description": "Retrieves a list of maps associated with the authenticated user account, including their metadata and status.",
      "Parameters": {
        "Query_Parameters": {
          "limit": {
            "Description": "The maximum number of maps to retrieve.",
            "Type": "integer",
            "Default": 10,
            "Required": false
          },
          "offset": {
            "Description": "The starting point for retrieving maps.",
            "Type": "integer",
            "Default": 0,
            "Required": false
          }
        }
      },
      "Responses": {
        "200": {
          "Description": "Successful response with a list of maps.",
          "Schema": {
            "maps": {
              "Description": "Array of map objects.",
              "Type": "array",
              "Items": {
                "Type": "object",
                "Properties": {
                  "map_id": {
                    "Description": "The unique identifier of the map.",
                    "Type": "string"
                  },
                  "name": {
                    "Description": "The name of the map.",
                    "Type": "string"
                  },
                  "status": {
                    "Description": "The status of the map (e.g., ACTIVE, PROCESSING).",
                    "Type": "string"
                  },
                  "created_at": {
                    "Description": "Timestamp when the map was created.",
                    "Type": "string"
                  }
                }
              }
            }
          },
          "Example_Response": {
            "maps": [
              {
                "map_id": "123e4567-e89b-12d3-a456-426614174000",
                "name": "Sample Map",
                "status": "ACTIVE",
                "created_at": "2023-06-01T12:00:00Z"
              }
            ]
          }
        },
        "422": {
          "Description": "Validation Error.",
          "Schema": {
            "detail": [
              {
                "loc": [
                  "string",
                  0
                ],
                "msg": "string",
                "type": "string"
              }
            ]
          },
          "Example_Response": {
            "detail": [
              {
                "loc": [
                  "string",
                  0
                ],
                "msg": "string",
                "type": "string"
              }
            ]
          }
        }
      }
    },
    "Examples": {
      "CURL": "curl -L 'https://api-atlas.nomic.ai/v1/maps?limit=10&offset=0' \\\n-H 'Authorization: Bearer <TOKEN>'",
      "Python": "import requests\n\nurl = \"https://api-atlas.nomic.ai/v1/maps\"\n\nparams = {\n  \"limit\": 10,\n  \"offset\": 0\n}\nheaders = {\n  'Authorization': 'Bearer <TOKEN>'\n}\n\nresponse = requests.request(\"GET\", url, headers=headers, params=params)\n\nprint(response.text)"
    }
  }
}