{
  "API_Documentation": {
    "Base_URL": "https://api-atlas.nomic.ai",
    "Authentication": {
      "Method": "Bearer Token",
      "Header_Example": {
        "Authorization": "Bearer <TOKEN>"
      }
    },
    "Endpoint": {
      "Name": "Embed Image",
      "Method": "POST",
      "Path": "/v1/query/embed-image",
      "Description": "Generates embeddings for an image file, which can be used for similarity search or other machine learning tasks.",
      "Parameters": {
        "Body_Parameters": {
          "image_file": {
            "Description": "The image file to embed. Must be in a supported format (e.g., JPEG, PNG).",
            "Type": "file",
            "Required": true
          }
        }
      },
      "Responses": {
        "200": {
          "Description": "Successful response with the image embedding.",
          "Schema": {
            "embedding": {
              "Description": "The embedding vector for the provided image.",
              "Type": "array",
              "Items": {
                "Type": "number"
              }
            }
          },
          "Example_Response": {
            "embedding": [
              0.123456,
              0.654321,
              0.987654
            ]
          }
        },
        "422": {
          "Description": "Validation Error.",
          "Schema": {
            "detail": [
              {
                "loc": [
                  "string",
                  0
                ],
                "msg": "string",
                "type": "string"
              }
            ]
          },
          "Example_Response": {
            "detail": [
              {
                "loc": [
                  "string",
                  0
                ],
                "msg": "string",
                  "type": "string"
              }
            ]
          }
        }
      }
    },
    "Examples": {
      "CURL": "curl -L 'https://api-atlas.nomic.ai/v1/query/embed-image' \\\n-H 'Authorization: Bearer <TOKEN>' \\\n-F 'image_file=@path/to/image.jpg'",
      "Python": "import requests\n\nurl = \"https://api-atlas.nomic.ai/v1/query/embed-image\"\n\nfiles = {\n  'image_file': open('path/to/image.jpg', 'rb')\n}\nheaders = {\n  'Authorization': 'Bearer <TOKEN>'\n}\n\nresponse = requests.request(\"POST\", url, headers=headers, files=files)\n\nprint(response.text)"
    }
  }
}